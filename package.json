{
  "name": "xtend-library",
  "version": "0.7.5",
  "description": "Frontend library",
  "homepage": "http://getxtend.com/",
  "license": "MIT",
  "author": "Riccardo Caroli <beaver82@gmail.com>",
  "repository": "https://github.com/minimit/xtend-library.git",
  "keywords": [
    "css",
    "html",
    "js",
    "front-end",
    "library",
    "plugin",
    "theme"
  ],
  "main": "dist/scripts/xtend.min.js",
  "umd:main": "dist/scripts/xtend.min.js",
  "umd:name": "xtend",
  "scripts": {
    "build": "gulp build",
    "dev": "gulp watch",
    "build:docs": "gulp build:docs",
    "dev:docs": "gulp watch:docs",
    "serve:docs": "bundle exec jekyll serve",
    "dep": "npm run dep:docs",
    "dep:docs": "gh-pages -d docs",
    "version": "gulp version",
    "release": "standard-version"
  },
  "dependencies": {
    "bezier-easing": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-for-of": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "fancy-log": "^1.3.2",
    "gh-pages": "^1.2.0",
    "gulp": "^4.0.0",
    "gulp-browserify": "^0.5.1",
    "gulp-cached": "^1.1.1",
    "gulp-clean-css": "^3.0.3",
    "gulp-less": "^4.0.1",
    "gulp-minify": "^3.1.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^0.6.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-terser": "^1.1.5",
    "gulp-util": "^3.0.8",
    "standard-version": "^5.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "standard-version": {
    "skip": {
      "tag": true
    },
    "scripts": {
      "postbump": "npm run version && npm run build:docs && git add . && git commit -m 'chore(release): compile' && npm run dep"
    }
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
