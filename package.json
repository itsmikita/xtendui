{
  "name": "xtend-library",
  "version": "0.13.8",
  "description": "Frontend library",
  "homepage": "http://getxtend.com/",
  "license": "MIT",
  "author": "Riccardo Caroli <beaver82@gmail.com>",
  "repository": "https://github.com/minimit/xtend-library.git",
  "keywords": [
    "frontend",
    "library",
    "components",
    "vanilla"
  ],
  "main": "dist/xtend.min.js",
  "module": "src/xt.js",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "dev": "gatsby clean && gatsby develop -p 8888",
    "serve": "npm run build && gatsby serve -p 8888",
    "deploy": "npm run build && gh-pages -d public",
    "release": "npx semantic-release --no-ci",
    "release:dry": "npx semantic-release --dry-run",
    "lint": "eslint src/** --fix",
    "prepare": "webpack --progress --colors"
  },
  "dependencies": {
    "bezier-easing": "^2.1.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.10",
    "gsap": "^3.0.5",
    "regenerator-runtime": "^0.13.3",
    "relaxed-json": "^1.0.3",
    "url-polyfill": "^1.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@hot-loader/react-dom": "^16.11.0",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/exec": "^3.3.2",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/release-notes-generator": "^7.3.4",
    "babel-loader": "^8.0.6",
    "babel-preset-gatsby": "^0.2.22",
    "clipboard": "^2.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "esm": "^3.2.25",
    "gatsby": "^2.18.2",
    "gatsby-image": "^2.2.33",
    "gatsby-plugin-catch-links": "^2.1.17",
    "gatsby-plugin-less": "^2.1.2",
    "gatsby-plugin-manifest": "^2.2.29",
    "gatsby-plugin-module-resolver": "^1.0.3",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-page-creator": "^2.1.31",
    "gatsby-plugin-react-helmet": "^3.1.15",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-remark-custom-blocks": "^2.1.18",
    "gatsby-source-filesystem": "^2.1.38",
    "gatsby-transformer-remark": "^2.6.37",
    "gh-pages": "^1.2.0",
    "glob": "^7.1.6",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lodash.kebabcase": "^4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier-eslint": "^9.0.1",
    "prism-themes": "^1.3.0",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "raw-loader": "^2.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.0",
    "rehype-react": "^3.1.0",
    "remark-grid-tables": "^1.7.1",
    "semantic-release": "^15.13.3",
    "terser-webpack-plugin": "^1.4.1",
    "webpack": "^5.0.0-beta.12",
    "webpack-cli": "^3.3.10",
    "write": "^1.0.3"
  },
  "resolutions": {
    "webpack-hot-middleware": "git://github.com/minimit/webpack-hot-middleware.git#fix-iframe"
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/exec",
        {
          "successCmd": "git pull && npm run deploy"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json",
            "_config.yml",
            "CHANGELOG.md",
            "dist/**/*.*",
            "src/**/*.*",
            "src/docs/assets/**/*.*"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
