{
  "name": "xtendui",
  "version": "1.0.0-beta.12",
  "description": "Custom frontend development with Vanilla Javascript and Tailwind Components.",
  "homepage": "http://xtendui.com/",
  "license": "MIT",
  "author": "Riccardo Caroli <beaver82+xtendui@gmail.com>",
  "repository": "https://github.com/minimit/xtendui.git",
  "keywords": [
    "react",
    "javascript",
    "components",
    "ui",
    "frontend",
    "react-components",
    "tailwindcss"
  ],
  "main": "src/xt.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "tailwind.*.js",
    "dist/*.min.*",
    "src/*.*"
  ],
  "scripts": {
    "clean": "gatsby clean",
    "build": "yarn packet && yarn clean && gatsby build",
    "dev": "yarn packet && yarn clean && gatsby develop -p 8080",
    "serve": "yarn build && gatsby serve -p 8080",
    "deploy": "yarn build && gh-pages -d public",
    "webpack": "yarn packet && webpack --progress",
    "release": "npx semantic-release --dry-run --no-ci",
    "packet": "node ./build/packet.js",
    "generate": "node ./build/generate.js && yarn lint --fix",
    "lint": "eslint . --ext .js,.jsx",
    "alias": "node -e \"var s='../src',d='node_modules/src',fs=require('fs');fs.exists(d,function(e){e||fs.symlinkSync(s,d,'junction')});\"",
    "postinstall": "yarn alias",
    "postuninstall": "yarn alias",
    "postversion": "yarn webpack"
  },
  "dependencies": {
    "@googlemaps/markerclustererplus": "^1.1.0",
    "@popperjs/core": "^2.9.2",
    "gsap": "^3.6.1",
    "json5": "^2.2.0",
    "tailwindcss": "^2.0.4"
  },
  "peerDependencies": {
    "tailwindcss": "2.x"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@hot-loader/react-dom": "^17.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.1.0",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "babel-preset-gatsby": "^1.2.0",
    "clipboard": "^2.0.8",
    "css-loader": "^5.2.0",
    "dotenv": "^8.2.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "esm": "^3.2.25",
    "gatsby": "^3.2.1",
    "gatsby-plugin-catch-links": "^3.2.0",
    "gatsby-plugin-manifest": "^3.2.0",
    "gatsby-plugin-module-resolver": "^1.0.3",
    "gatsby-plugin-page-creator": "^3.2.0",
    "gatsby-plugin-postcss": "^4.2.0",
    "gatsby-plugin-react-helmet": "^4.2.0",
    "gatsby-plugin-sitemap": "^3.2.0",
    "gatsby-remark-classes": "^1.0.0",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-remark-custom-blocks": "^3.2.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-source-contentful": "5.2.0",
    "gatsby-source-filesystem": "^3.2.0",
    "gatsby-source-prismic": "^3.3.4",
    "gatsby-transformer-remark": "^3.2.0",
    "gh-pages": "^3.1.0",
    "glob": "^7.1.6",
    "lodash.castarray": "^4.4.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.merge": "^4.6.2",
    "mini-css-extract-plugin": "^1.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "postcss": "^8.2.9",
    "postcss-import": "^14.0.1",
    "postcss-loader": "^5.2.0",
    "postcss-nested": "^5.0.5",
    "prettier-eslint": "^12.0.0",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "rehype-react": "^6.2.0",
    "remark-grid-tables": "^2.1.2",
    "semantic-release": "^17.4.2",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0",
    "write": "^2.0.0"
  }
}
