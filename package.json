{
  "name": "xtendui",
  "version": "1.0.0-beta.30",
  "description": "Xtend UI is a powerfull library to create advanced UI with custom tailwind components and advanced interactions and animations.",
  "homepage": "https://xtendui.com/",
  "license": "Apache-2.0",
  "author": "Riccardo Caroli <beaver82+xtendui@gmail.com>",
  "repository": "https://github.com/minimit/xtendui.git",
  "keywords": [
    "react",
    "javascript",
    "components",
    "ui",
    "frontend",
    "react-components",
    "tailwindcss"
  ],
  "main": "src/xt.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "tailwind.*.js",
    "dist/*.min.*",
    "src/*.*"
  ],
  "scripts": {
    "clean": "gatsby clean",
    "setup": "yarn alias && node ./build/packet.js",
    "generate": "yarn alias && node ./build/generate.js && yarn lint:generate --fix",
    "build": "yarn setup && yarn clean && gatsby build",
    "dev": "yarn setup && yarn clean && gatsby develop -p 8080",
    "serve": "yarn build && gatsby serve -p 8080",
    "deploy": "yarn build && gh-pages -d public",
    "webpack": "yarn setup && webpack --progress",
    "release": "npx semantic-release --dry-run --no-ci",
    "lint": "eslint . --ext .js",
    "lint:generate": "eslint . --ext .jsx",
    "test": "echo '@todo tests'",
    "alias": "node -e \"var s='../src',d='node_modules/src',fs=require('fs');fs.exists(d,function(e){e||fs.symlinkSync(s,d,'junction')});\"",
    "postversion": "yarn webpack"
  },
  "dependencies": {
    "@googlemaps/markerclustererplus": "^1.2.0",
    "@popperjs/core": "^2.9.2",
    "gsap": "^3.6.1",
    "relaxed-json": "^1.0.3",
    "tailwindcss": "^2.2.0"
  },
  "peerDependencies": {
    "tailwindcss": "2.x"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@hot-loader/react-dom": "^17.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "babel-preset-gatsby": "^1.7.0",
    "clipboard": "^2.0.8",
    "css-loader": "^5.2.6",
    "cssnano": "^5.0.5",
    "dotenv": "^10.0.0",
    "eslint": "7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "esm": "^3.2.25",
    "gatsby": "^3.7.0",
    "gatsby-plugin-catch-links": "^3.7.0",
    "gatsby-plugin-image": "^1.7.0",
    "gatsby-plugin-manifest": "^3.7.0",
    "gatsby-plugin-module-resolver": "^1.0.3",
    "gatsby-plugin-page-creator": "^3.7.0",
    "gatsby-plugin-postcss": "^4.7.0",
    "gatsby-plugin-react-helmet": "^4.7.0",
    "gatsby-plugin-sitemap": "^4.3.0",
    "gatsby-remark-classes": "^1.0.0",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-remark-custom-blocks": "^3.7.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-source-contentful": "5.7.0",
    "gatsby-source-filesystem": "^3.7.0",
    "gatsby-source-prismic": "^3.3.6",
    "gatsby-transformer-remark": "^3.2.0",
    "gh-pages": "^3.2.0",
    "glob": "^7.1.7",
    "lodash.castarray": "^4.4.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.merge": "^4.6.2",
    "mini-css-extract-plugin": "^1.6.0",
    "path": "^0.12.7",
    "postcss": "^8.3.1",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^5.3.0",
    "prettier-eslint": "^12.0.0",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "rehype-react": "^6.2.0",
    "remark-grid-tables": "^2.1.2",
    "semantic-release": "^17.4.3",
    "terser-webpack-plugin": "^5.1.3",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "write": "^2.0.0"
  }
}
